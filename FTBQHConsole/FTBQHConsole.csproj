<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <OutputType>Exe</OutputType>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    
    <PackageProjectUrl>https://github.com/sunny1028/FTBQuests-Hanize</PackageProjectUrl>
    <RepositoryUrl>https://github.com/sunny1028/FTBQuests-Hanize</RepositoryUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <Description>该项目利用AI翻译工具实现自动 汉化 minecraft 的 FTB任务</Description>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>

</Project>
